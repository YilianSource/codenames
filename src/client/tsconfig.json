{
    "compilerOptions": {
        "lib": [
            "DOM",
            "ES2015"
        ],
        "target": "ES6",
        "moduleResolution": "node",
        "jsx": "react",
        "jsxFactory": "React.createElement",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "incremental": true,
        "forceConsistentCasingInFileNames": true
    }
}